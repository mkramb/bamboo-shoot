{
  "name": "BambooShoot",
  "version": "1.0.0.",
  "dependencies": {
    "angular": "~1.3.14",
    "angular-animate": "~1.3.14",
    "angular-ui-router": "~0.2.13",
    "angular-mocks": "~1.3.15",
    "angular-mousetrap": "~1.0.1",
    "mousetrap": "~1.5.2",
    "jquery": "~2.1.4"
  },
  "install": {
    "base": "public/lib",
    "path": {
      "js": "{name}/js",
      "css": "{name}/css",
      "eot": "{name}/fonts",
      "svg": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts",
      "woff2": "{name}/fonts",
      "otf": "{name}/fonts"
    },
    "sources": {
      "jquery": "bower_components/jquery/dist/jquery.min.js",
      "angular": "bower_components/angular/angular.min.js",
      "angular-animate": "bower_components/angular-animate/angular-animate.min.js",
      "angular-ui-router": "bower_components/angular-ui-router/release/angular-ui-router.min.js",
      "angular-mocks": "bower_components/angular-mocks/angular-mocks.js",
      "angular-mousetrap": "bower_components/angular-mousetrap/src/keypress.js",
      "mousetrap": [
        "bower_components/mousetrap/mousetrap.min.js",
        "bower_components/mousetrap/plugins/global-bind/mousetrap-global-bind.js"
      ]
    }
  }
}
